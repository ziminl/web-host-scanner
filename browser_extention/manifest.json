


{
  "manifest_version": 2,
  "name": "Port Scanner",
  "version": "1.0",
  "description": "Scan open ports from 1 to 2000",
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}


